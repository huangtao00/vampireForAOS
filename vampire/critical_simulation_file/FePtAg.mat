material:num-materials=2
#---------------------------------------------------
# Material 1 Fe first layer
#--------------------------------------------------
material[1]:core-shell-size = 0.8
material[2]:core-shell-size = 1


material[1]:material-name=TM
material[1]:damping-constant=1
material[1]:exchange-matrix[1]=3.5e-21
material[1]:exchange-matrix[2]=3.5e-21
#material[1]:atomic-spin-moment=2.22 !muB  1.66
material[1]:atomic-spin-moment=3.2 !muB
material[1]:uniaxial-anisotropy-constant=7.3219471534e-24
 
#material[1]:uniaxial-anisotropy-direction=0,0,1
material[1]:material-element=Fe
material[1]:initial-spin-direction=0,0,1


material[2]:material-name=Ag
material[2]:damping-constant=1
material[2]:exchange-matrix[1]=3.5e-21
material[2]:exchange-matrix[2]=3.5e-21
#material[2]:atomic-spin-moment=2.22 !muB  1.66
material[2]:atomic-spin-moment=3.2 !muB
material[2]:uniaxial-anisotropy-constant=7.3219471534e-24
 
#material[2]:uniaxial-anisotropy-direction=0,0,1
material[2]:material-element=Fe
material[2]:initial-spin-direction=0,0,1
