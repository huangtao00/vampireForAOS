install_target: first FORCE
	@test -d $(INSTALL_ROOT)/opt/vampire_qt/bin || mkdir -p $(INSTALL_ROOT)/opt/vampire_qt/bin
	-$(INSTALL_PROGRAM) $(QMAKE_TARGET) $(INSTALL_ROOT)/opt/vampire_qt/bin/$(QMAKE_TARGET)

uninstall_target: FORCE
	-$(DEL_FILE) $(INSTALL_ROOT)/opt/vampire_qt/bin/$(QMAKE_TARGET)
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/vampire_qt/bin/ 


